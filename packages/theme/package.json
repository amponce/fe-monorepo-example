{
  "name": "@otus/theme",
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build.watch": "ng build --watch",
    "clean": "rimraf dist node_modules",
