{
  "name": "@otus/components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "clean": "rimraf dist",
    "test": "ng test",
    "test.watch": "ng test --watch",
    "test.coverage": "ng test --coverage",
    "test.screenshots": "ENABLE_SCREENSHOTS=1 ng test --configuration=screenshots"
  },
  "devDependencies": {
    "@otus/test-utils": "^0.1.0",
    "@otus/svg-icons": "^0.1.0",
    "@otus/theme": "^0.1.0"
  },
  "dependencies": {
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26"
  },
  "peerDependencies": {
    "@angular/common": "^10",
    "@angular/core": "^10",
    "@otus/svg-icons": "^0",
    "@otus/theme": "^0",
    "lodash-es": "^4",
    "rxjs": "^6"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "ngPackage": {
    "dest": "dist",
    "whitelistedNonPeerDependencies": [
      "@fortawesome/angular-fontawesome",
      "@fortawesome/fontawesome-svg-core"
    ],
    "lib": {
      "entryFile": "src/index.ts"
    }
  }
}
